{
  "name": "node_template",
  "version": "1.0.0",
  "description": "Node.js project template",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node build/index.js",
    "build": "rimraf ./build && tsc",
    "lint": "eslint \"src/**\" --quiet --fix",
    "lint-ci": "eslint \"src/**\" --quiet",
    "test": "echo Specify a test script in package.json to run tests",
    "prepare": "husky install",
    "update-latest": "ncu -u && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "npm run lint && ts-node ./src/index.ts"
  }
}
