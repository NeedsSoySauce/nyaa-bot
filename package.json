{
  "name": "node_template",
  "version": "1.0.0",
  "description": "Node.js project template",
  "exports": "./index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "node build/index.js",
    "build": "rimraf ./build && tsc",
    "lint": "eslint \"src/**\" --quiet --fix",
    "lint-ci": "eslint \"src/**\" --quiet",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "update-latest": "ncu -u && npm install",
    "deploy-commands": "node --loader ts-node/esm -r dotenv/config ./src/index.ts dotenv_config_path=.env deploy-commands"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/collection": "^1.0.0",
    "@discordjs/rest": "^1.0.0",
    "discord-api-types": "^0.36.2",
    "discord.js": "^14.0.3",
    "fast-xml-parser": "^4.0.9",
    "mongodb": "^4.8.0",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-check-updates": "^15.3.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "node --loader ts-node/esm -r dotenv/config ./src/index.ts dotenv_config_path=.env"
  }
}
