{
  "name": "node_template",
  "version": "1.0.0",
  "description": "Node.js project template",
  "exports": "./index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "start": "node build/index.js",
    "build": "rimraf ./build && tsc",
    "lint": "eslint \"src/**\" --quiet --fix",
    "lint-ci": "eslint \"src/**\" --quiet",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepare": "husky install",
    "update-latest": "ncu -u && npm install",
    "deploy-commands": "node --loader ts-node/esm -r dotenv/config ./src/index.ts dotenv_config_path=.env deploy-commands"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.28",
    "discord.js": "^14.7.1",
    "fast-xml-parser": "^4.0.13",
    "mongodb": "^4.13.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.6",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-node": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.6.2",
    "rimraf": "^4.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "node --loader ts-node/esm -r dotenv/config ./src/index.ts dotenv_config_path=.env"
  }
}
